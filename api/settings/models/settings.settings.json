{
  "kind": "singleType",
  "collectionName": "settings",
  "info": {
    "name": "Settings"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "LogoText": {
      "type": "string",
      "default": "Logo",
      "maxLength": 8,
