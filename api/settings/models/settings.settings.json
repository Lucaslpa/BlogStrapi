    "LogoImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Options": {
      "type": "component",
      "repeatable": true,
      "component": "menu.options",
      "required": true
    }
  }
}
